{
  "env": {
    "mocha": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "globals": {
    "artifacts": false,
    "contract": false,
    "web3": false,
    "assert": false
  },
  "plugins": [
    "mocha"
  ],
  "rules": {
    "mocha/no-exclusive-tests": "error",
    "import/no-named-default": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
